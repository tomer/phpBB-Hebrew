#: $lang->'REASON_REMOVED'
msgid "Report/denial reason successfully removed."
msgstr "סיבת הדיווח נמחקה בהצלחה."

#: $lang->'REASON_TITLE'
msgid "Reason title"
msgstr "כותרת הסיבה"

#: $lang->'REASON_TITLE_TRANSLATED'
msgid "Displayed reason title"
msgstr "כותרת הסיבה המוצגת"

#: $lang->'REASON_UPDATED'
