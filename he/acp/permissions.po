msgstr "הרשאות מתקדמות"

#: $lang->'ALL_GROUPS'
msgid "Select all groups"
msgstr "בחר את כל הקבוצות"

#: $lang->'ALL_NEVER'
msgid "All <strong>NEVER</strong>"
msgstr "הכול <strong>לעולם לא</strong>"

#: $lang->'ALL_NO'
msgid "All <strong>NO</strong>"
msgstr "הכול <strong>לא</strong>"

#: $lang->'ALL_USERS'
msgid "Select all users"
msgstr "בחר את כל המשתמשים"

#: $lang->'ALL_YES'
msgid "All <strong>YES</strong>"
msgstr "הכול <strong>כן</strong>"

