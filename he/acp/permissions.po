msgstr "חצי ניהול"

#: $lang->'ROLE_FORUM_POLLS'
msgid "Standard Access + Polls"
msgstr "הרשאה רגילה + סקרים"

#: $lang->'ROLE_FORUM_READONLY'
msgid "Read Only Access"
msgstr "הרשאה לקריאה בלבד"

#: $lang->'ROLE_FORUM_STANDARD'
msgid "Standard Access"
msgstr "הרשאה רגילה"

#: $lang->'ROLE_FORUM_NEW_MEMBER'
msgid "Newly Registered User Access"
msgstr "הרשאת משתמש שנרשם לאחרונה"

#: $lang->'ROLE_MOD_FULL'
msgid "Full Moderator"
msgstr "מנהל מלא"

#: $lang->'ROLE_MOD_QUEUE'
